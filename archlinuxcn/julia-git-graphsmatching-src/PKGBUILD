pkgname=julia-git-graphsmatching-src
pkgver=0.2.0
_commit=7d14c0b345eb701f86ff20236e26a8a050994932
pkgrel=1
pkgdesc="GraphsMatching.jl"
url="https://github.com/JuliaGraphs/GraphsMatching.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
