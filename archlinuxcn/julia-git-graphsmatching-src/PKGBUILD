sha256sums=('SKIP')

package() {
  cd GraphsMatching.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh GraphsMatching "${pkgdir}" "${pkgname}" julia-git
}
